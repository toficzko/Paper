Subject: [PATCH] PlayerTeleportEndGatewayEvent

Allows you to access the Gateway being used in a teleport event

diff --git a/src/main/java/io/papermc/paper/event/player/PlayerTeleportEndGatewayEvent.java b/src/main/java/io/papermc/paper/event/player/PlayerTeleportEndGatewayEvent.java
new file mode 100644
index 0000000000000000000000000000000000000000..de78a4295b4620407642fd5b5a995acd6501cfbc
--- /dev/null
+++ b/src/main/java/io/papermc/paper/event/player/PlayerTeleportEndGatewayEvent.java
@@ -0,0 +1,29 @@
+package io.papermc.paper.event.player;
+
+import org.bukkit.Location;
+import org.bukkit.block.EndGateway;
+import org.bukkit.entity.Player;
+import org.bukkit.event.player.PlayerTeleportEvent;
+import org.jetbrains.annotations.NotNull;
+
+/**
+ * Fired when a teleport is triggered for an End Gateway
+ */
+public class PlayerTeleportEndGatewayEvent extends PlayerTeleportEvent {
+    @NotNull private final EndGateway gateway;
+
+    public PlayerTeleportEndGatewayEvent(@NotNull Player player, @NotNull Location from, @NotNull Location to, @NotNull EndGateway gateway) {
+        super(player, from, to, PlayerTeleportEvent.TeleportCause.END_GATEWAY);
+        this.gateway = gateway;
+    }
+
+    /**
+     * The gateway triggering the teleport
+     *
+     * @return EndGateway used
+     */
+    @NotNull
+    public EndGateway getGateway() {
+        return gateway;
+    }
+}
